CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False

Option Compare Database
Option Explicit

Private Sub btnSalesOrder_Click()
    ' SALES flow: open SalesOrderEntry directly with SALES flag
    DoCmd.OpenForm "frmSalesOrderEntry", , , , acFormAdd, , "SALES"
    DoCmd.Close acForm, Me.name
End Sub

Private Sub btnProjectOrder_Click()
    On Error GoTo EH

    ' Clear any stale selection (defensive across Access versions)
    Dim tv As TempVars
    Set tv = Application.TempVars
    On Error Resume Next
    tv.Remove "SelectedBaseToken"
    On Error GoTo EH

    ' Open the selection dialog modally
    DoCmd.OpenForm "dlgProjectSelection", , , , , acDialog

    ' Code resumes only after dlgProjectSelection is closed.
    Dim selBase As String
    selBase = ""
    On Error Resume Next
    selBase = Nz(tv("SelectedBaseToken").Value, "")
    On Error GoTo EH

    If Len(selBase) > 0 Then
        DoCmd.OpenForm "frmSalesOrderEntry", , , , acFormAdd, , "PROJECT:" & selBase
    End If

    ' Clean up and close the chooser
    On Error Resume Next
    tv.Remove "SelectedBaseToken"
    DoCmd.Close acForm, Me.name
    Exit Sub

EH:
    MsgBox "Error opening project selection: " & Err.Description, vbExclamation
End Sub


