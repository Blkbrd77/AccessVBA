----------------------------------------
TABLE: Customers (3797 rows)

  Fields:
    - CustomerID : Long
    - CustomerName : Text(255)
    - CustomerCode : Text(20)
    - IsActive : Yes/No DEFAULT No

  Primary Key: CustomerID
  Indexes:
    - CustomerCode: CustomerCode
    - CustomerID: CustomerID

----------------------------------------
TABLE: OrderSeq (13 rows)

  Fields:
    - Scope : Text(10)
    - BaseToken : Text(32)
    - QualifierCode : Text(10)
    - SystemLetter : Text(1)
    - NextSeq : Long
    - LastUpdated : Date/Time

  Primary Key: (none)
  Indexes:
    - ix_OrderSeq_BaseToken: BaseToken
    - ux_OrderSeq_Domain: Scope, BaseToken, QualifierCode, SystemLetter [UNIQUE]

----------------------------------------
TABLE: Projects (8 rows)

  Fields:
    - ProjectID : Long
    - BaseToken : Text(20)
    - Program : Text(255)
    - JobCode3 : Text(255)
    - ActiveFlag : Yes/No DEFAULT =Yes

  Primary Key: ProjectID
  Indexes:
    - ProjectID: ProjectID

----------------------------------------
TABLE: QualifierType (10 rows)

  Fields:
    - QCID : Long
    - QualifierCode : Text(10)
    - QualifierName : Text(100)
    - SequenceWidth : Long DEFAULT 0
    - AllowMultiple : Yes/No DEFAULT No
    - ActiveFlag : Yes/No DEFAULT No
    - Notes : Memo

  Primary Key: QCID
  Indexes:
    - QCID: QCID
    - QualifierCode: QualifierCode
    - ux_QualifierType_Code: QualifierCode [UNIQUE]

----------------------------------------
TABLE: SalesOrderEntry (36 rows)

  Fields:
