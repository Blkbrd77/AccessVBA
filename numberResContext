    - SOID : Long DEFAULT 0
    - OrderNumberDisplay : Text(30)
    - QualifierCode : Text(10)
    - SequenceNo : Integer DEFAULT 0
    - IsDeleted : Yes/No DEFAULT No
    - CreatedOn : Date/Time

  Primary Key: SOEntryID
  Indexes:
    - fk_SOEntry_SOID: SOID
    - ix_SOEntry_SOID: SOID
    - QualifierCode: QualifierCode
    - SOEntryID: SOEntryID
    - SOID: SOID

