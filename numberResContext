CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False

Option Compare Database
Option Explicit

'===========================================
' Paging variables
'===========================================
Private Const PAGE_SIZE As Long = 25
Private mlngStartRow As Long
Private mlngEndRow As Long

Private Sub cmdReserveNumbers_Click()

   On Error GoTo EH

    Dim bt As String
    bt = ""
    On Error Resume Next
    bt = Nz(Me!BaseToken, "")
    On Error GoTo EH

    If Len(bt) > 0 Then
        DoCmd.OpenForm "dlgBatchGenerateOrders", , , , , acDialog, bt
    Else
        DoCmd.OpenForm "dlgBatchGenerateOrders", , , , , acDialog
    End If

    ' After wizard closes, refresh the list (in case it filtered to BatchID)
    Me.Requery
    Exit Sub

EH:
    MsgBox "Open wizard failed: " & Err.Description, vbExclamation

End Sub

Private Sub Form_Open(Cancel As Integer)
    mlngStartRow = 1
    mlngEndRow = PAGE_SIZE
End Sub

Private Sub Form_Load()
    RefreshPage
End Sub

Private Sub cmdNewOrder_Click()
    ' Open chooser (Sales vs Project)
    DoCmd.OpenForm "dlgNewOrderType", , , , acFormAdd
End Sub

Private Sub cmdNextPage_Click()
    mlngStartRow = mlngEndRow + 1
    mlngEndRow = mlngEndRow + PAGE_SIZE
    RefreshPage
End Sub

Private Sub cmdPrevPage_Click()
    mlngStartRow = mlngStartRow - PAGE_SIZE
    If mlngStartRow < 1 Then mlngStartRow = 1
    mlngEndRow = mlngStartRow + PAGE_SIZE - 1
    RefreshPage
End Sub

Private Sub RefreshPage()
    Dim sql As String
    sql = ""
    sql = sql & "SELECT * FROM ("
    sql = sql & "  SELECT "
    sql = sql & "    Q.*, "
    sql = sql & "    DCount('*', 'SalesOrders', 'SOID >= ' & Q.SOID) AS RowNum "
    sql = sql & "  FROM qryOrderList_Base AS Q "
    sql = sql & ") AS X "
    sql = sql & "WHERE X.RowNum BETWEEN " & mlngStartRow & " AND " & mlngEndRow & " "
    sql = sql & "ORDER BY X.SOID DESC;"
    Me.RecordSource = sql
    Me.Requery
    Me.caption = "Order List Rows " & mlngStartRow & " to " & mlngEndRow
End Sub


