    - BaseToken : Text(50)
    - SystemLetter : Text(1)
    - BackorderNo : Integer
    - CustomerName : Text(255)
    - CustomerCode : Text(255)
    - PONumber : Text(55)
    - DateReceived : Date/Time
    - DateBilled : Date/Time
    - ActiveFlag : Yes/No DEFAULT =Yes
    - OrderNumber : Text(50)
    - DateBackorderCreated : Date/Time
    - DateCreated : Date/Time
    - DateCanceled : Date/Time
    - CancelReason : Text(255)
    - CanceledBy : Text(64)
    - BatchID : Text(36)

  Primary Key: SOID
  Indexes:
    - ix_SO_PONumber: PONumber
    - SystemLetterSalesOrders: SystemLetter
    - ux_SO_OrderNumber: OrderNumber [UNIQUE]

----------------------------------------
