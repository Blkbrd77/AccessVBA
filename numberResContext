CodeBehindForm
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False

Option Compare Database
Option Explicit

Private Sub Form_Open(Cancel As Integer)
    On Error GoTo EH

    ' Recommended design-time props: Modal=Yes, PopUp=Yes, Auto Center=Yes

    ' Populate the combo
    Me.cboProjectSelect.RowSource = _
        "SELECT BaseToken, Program, JobCode3 " & _
        "FROM Projects WHERE ActiveFlag=True ORDER BY BaseToken;"

    ' Combo (set in design):
    '   Column Count = 3
    '   Bound Column = 1  (BaseToken)
    '   Column Widths = 1.2"";2.5"";1""
    '   Limit To List = Yes

    Me.cboProjectSelect = Null
    Me.btnNext.Enabled = False
    Exit Sub

EH:
    MsgBox "dlgProjectSelection Open error: " & Err.Description, vbExclamation
End Sub

Private Sub cboProjectSelect_AfterUpdate()
    Me.btnNext.Enabled = (Nz(Me.cboProjectSelect.Value, "") <> "")
End Sub

Private Sub btnNext_Click()
    On Error GoTo EH

    Dim projectBase As String
    projectBase = Nz(Me.cboProjectSelect.Value, "")

    If Len(projectBase) = 0 Then
        MsgBox "Please select a project.", vbInformation
        Exit Sub
    End If

    ' Return the value for the caller
    Dim tv As TempVars
    Set tv = Application.TempVars
    On Error Resume Next: tv.Remove "SelectedBaseToken": On Error GoTo EH
    tv.Add "SelectedBaseToken", projectBase

    DoCmd.Close acForm, Me.name
    Exit Sub

EH:
    MsgBox "Selection error: " & Err.Description, vbExclamation
End Sub

Private Sub btnCancel_Click()
    On Error Resume Next
    Application.TempVars.Remove "SelectedBaseToken"
    DoCmd.Close acForm, Me.name
End Sub


================================================================
REPORTS
================================================================

(No reports in database)

