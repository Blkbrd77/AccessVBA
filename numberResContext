================================================================
QUERIES
================================================================

--- QUERY: dlgNewOrderType ---
Type: Select
SQL:
SELECT Projects.BaseToken, Projects.Program, Projects.JobCode3
FROM Projects;


--- QUERY: frmOrderList ---
Type: Select
SQL:
SELECT S.SOID, S.OrderNumber, S.CustomerName, S.PONumber, S.DateReceived, F.QualifierCode, F.SequenceNo, S.BaseToken, S.SystemLetter, S.BackorderNo
FROM SalesOrders AS S LEFT JOIN qryFirstQualifierPerSO AS F ON S.SOID = F.SOID
WHERE S.ActiveFlag = True
ORDER BY S.SOID DESC;


--- QUERY: frmSalesOrderEntry ---
Type: Select
SQL:
SELECT SystemLetter, Description
FROM SystemLetter
WHERE ActiveFlag = True
ORDER BY SystemLetter;


--- QUERY: fsubQualifierQty ---
Type: Select
SQL:
SELECT QualifierCode, Description, Qty
FROM tmpQualifierQty
ORDER BY QualifierCode;


--- QUERY: qryFirstQualifierOnly ---
Type: Select
SQL:
SELECT SOID, QualifierCode, SequenceNo, OrderNumberDisplay
FROM SalesOrderEntry
WHERE IsDeleted = 0
  AND QualifierCode Is Not Null
  AND QualifierCode <> ''
  AND SequenceNo = (
        SELECT MIN(E2.SequenceNo)
        FROM SalesOrderEntry AS E2
        WHERE E2.SOID = SalesOrderEntry.SOID
          AND E2.IsDeleted = 0
          AND E2.QualifierCode Is Not Null
          AND E2.QualifierCode <> ''
    );


--- QUERY: qryOrderList_Base ---
Type: Select
SQL:
SELECT S.SOID, S.OrderNumber, S.CustomerName, S.CustomerCode, S.PONumber, S.DateReceived, S.DateBilled, S.BaseToken, S.SystemLetter, S.BackorderNo, Q.QualifierCode, Q.SequenceNo
FROM SalesOrders AS S LEFT JOIN qryFirstQualifierOnly AS Q ON S.SOID = Q.SOID
WHERE S.ActiveFlag = True;


